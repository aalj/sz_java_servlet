    <servlet-class>com.snail.ToVerifyCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ToVerifyCodeServlet</servlet-name>
    <url-pattern>/ToVerifyCodeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AvoidReSubmitServlet</display-name>
