    <servlet-class>com.snail.AvoidReSubmitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AvoidReSubmitServlet</servlet-name>
    <url-pattern>/AvoidReSubmitServlet</url-pattern>
  </servlet-mapping>
</web-app>